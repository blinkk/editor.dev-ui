{
  "name": "@blinkk/editor.dev-ui",
  "version": "1.3.3",
  "license": "MIT",
  "homepage": "https://blinkk.github.com/editor.dev-ui",
  "description": "Structured content editor with live previews.",
  "keywords": [
    "structured",
    "document",
    "ui",
    "editor",
    "live"
  ],
  "bugs": {
    "url": "https://github.com/blinkk/editor.dev-ui/issues"
  },
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkk/editor.dev-ui.git"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/src",
    "dist/css"
  ],
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "npx tsc --skipLibCheck && npx gulp compile && webpack --config webpack.prod.js",
    "coverage": "codecov",
    "dev": "concurrently \"npx ava --watch\" \"tsc --watch\"",
    "docs": "typedoc",
    "fix": "npx eslint --fix src/**/*.ts",
    "hosted": "concurrently \"webpack --config webpack.server.js\" \"npx gulp watch\" \"nodemon ./src/server/server.ts\"",
    "serve": "concurrently \"webpack serve --config webpack.dev.js\" \"npx gulp watch\"",
    "test": "nyc ava",
    "test-ci": "nyc --reporter=lcov ava && codecov"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "@types/async": "^3.2.6",
    "@types/bent": "^7.3.2",
    "@types/express": "^4.17.12",
    "@types/javascript-time-ago": "^2.0.2",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash.merge": "^4.6.6",
    "@types/marked": "^2.0.3",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^15.12.2",
    "@types/nunjucks": "^3.1.4",
    "@types/quill": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "ava": "^3.14.0",
    "codecov": "^3.8.2",
    "concurrently": "^6.2.0",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "express": "^4.17.1",
    "gts": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "nodemon": "^2.0.7",
    "nunjucks": "^3.2.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.36",
    "typescript": "^4.3.2",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@blinkk/selective-edit": "^1.2.0",
    "@toast-ui/editor": "^2.5.2",
    "bent": "^7.3.12",
    "codemirror": "^5.61.1",
    "javascript-time-ago": "^2.3.6",
    "js-yaml": "^4.1.0",
    "lodash.merge": "^4.6.2",
    "marked": "^2.1.1",
    "quill": "^1.3.7",
    "stackdriver-errors-js": "^0.8.0"
  }
}
